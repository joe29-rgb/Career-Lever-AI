{
  "output": {
    "filePath": "COMPLETE-FULL-ANALYSIS.xml",
    "style": "xml"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "*.json",
    "*.config.js",
    "*.config.ts",
    "vercel.json",
    ".env.example"
  ],
  "ignore": {
    "useDefaultPatterns": true,
    "customPatterns": [
      "*.xml",
      "*.md",
      "node_modules/**",
      ".next/**",
      "dist/**",
      "build/**"
    ]
  }
}
